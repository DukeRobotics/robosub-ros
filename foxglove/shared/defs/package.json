{
  "name": "@duke-robotics/defs",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf custom_msg_defs/dist/* datatype_maps/dist/* types/dist/*",
    "build-ts": "npx ts-node src/index.ts",
    "generate-types": "npx ros-typescript-generator --config custom-msgs-ts-generator-config.json",
    "concatenate-types": "cat types/dist/custom_msgs.ts types/ros1_msgs.ts > types/dist/types.ts",
    "build": "npm run clean && npm run build-ts && npm run generate-types && npm run concatenate-types"
  },
  "exports": {
    "./datatype_maps": {
      "import": "./datatype_maps/dist/index.js",
      "types": "./datatype_maps/dist/index.d.ts"
    },
    "./custom_msg_defs": {
      "import": "./custom_msg_defs/dist/index.js",
      "types": "./custom_msg_defs/dist/index.d.ts"
    }
  },
  "devDependencies": {
    "cross-cat": "^1.0.0"
  }
}