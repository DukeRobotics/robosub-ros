<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node" output="screen" >
        <param
            name="port"
            command = "$(find offboard_comms)/scripts/thruster_port.sh"
        />
    </node>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node2" output="screen" >
	<param
	    name="port"
	    command = "$(find offboard_comms)/scripts/pressure_port.sh"
	/>
    </node>
    <node pkg="offboard_comms" type="servo_wrapper.py" name="servo_wrapper" output="screen" />
</launch>
